{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../../../dist/out-tsc",
    "types": ["node"],
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "files": [
    "../../../../node_modules/@nx/react/typings/cssmodule.d.ts",
    "../../../../node_modules/@nx/react/typings/image.d.ts"
